   "source": [
    "# Add first k elements, then next k elements, etc.\n",
    "k = 4\n",
    "arr = [1,1,0,2, 3,5,2,2, 7,6,4,3]\n",
    "\n",
    "# Solution\n",
    "n = len(arr)\n",
    "m = n//k if n%k==0 else n//k + 1\n",
    "res = [0] * m\n",
    "for i in range(n):\n",
    "    res[i//k] += arr[i]\n",
    "print(res)"
   ]
