    "class Solution:\n",
    "    def singleNumber(self, nums):\n",
    "        h = {}\n",
    "        for x in nums:\n",
    "            if x not in h:\n",
    "                h[x] = 1\n",
    "            else:\n",
    "                del h[x]\n",
    "        print(h)\n",
    "        return list(h.keys())[0]"
   ]
